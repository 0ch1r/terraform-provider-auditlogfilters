# Environment variables for local development
# Copy this file to .env and fill in your values

# MySQL connection settings for testing
MYSQL_ENDPOINT=localhost:3306
MYSQL_USERNAME=root
MYSQL_PASSWORD=your_password_here
MYSQL_DATABASE=mysql
MYSQL_TLS=false

# Terraform acceptance test settings
TF_ACC=1

# Debugging (optional - set to DEBUG for detailed logs)
TF_LOG=INFO
TF_LOG_PROVIDER=INFO

# GoReleaser settings (for releases)
# GITHUB_TOKEN=your_github_token_here
# GPG_FINGERPRINT=your_gpg_fingerprint_here

# Personal customization examples
# Add your own variables below this line
# MY_CUSTOM_VAR=value
